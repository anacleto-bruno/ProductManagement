{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  },
  "extensions": {
    "http": {
      "customHeaders": {
        "X-Powered-By": "ProductManagement API"
      }
    }
  },
  "cors": {
    "allowCredentials": false,
    "allowedOrigins": [
      "*"
    ]
  }
}